# ============================================
# OpenClaw Agents Platform - Environment Variables
# ============================================
# Copy this file to .env and fill in your values
# cp .env.example .env

# === AI Services ===
OPENROUTER_API_KEY=sk-or-your-api-key-here
FAL_KEY=your-fal-ai-key-here

# === Social / Farcaster ===
NEYNAR_API_KEY=your-neynar-api-key-here
NEYNAR_WEBHOOK_SECRET=your-neynar-webhook-secret-here

# === Blockchain (Base) ===
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
DEPLOYER_PRIVATE_KEY=0xyour-deployer-private-key-here

# === x402 Payment Protocol ===
X402_FACILITATOR_URL=https://x402.coinbase.com
X402_RESOURCE_WALLET=0xyour-resource-wallet-here
NEXT_PUBLIC_USDC_CONTRACT=0x036CbD53842c5426634e7929541eC2318f3dCF7e

# === Coinbase CDP ===
COINBASE_CDP_API_KEY=your-cdp-api-key-here
COINBASE_CDP_API_SECRET=your-cdp-api-secret-here

# === Database ===
DATABASE_URL=postgresql://openclaw:openclaw@localhost:5432/openclaw?schema=public

# === Redis (BullMQ) ===
REDIS_URL=redis://localhost:6379

# === App Config ===
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your-walletconnect-project-id
NEXT_PUBLIC_CHAIN_ID=84532
NEXT_PUBLIC_DEMO_MODE=true

# === Contract Addresses (Base Sepolia - populated after deploy) ===
NEXT_PUBLIC_FACTORY_ADDRESS=
NEXT_PUBLIC_REGISTRY_ADDRESS=
NEXT_PUBLIC_REVENUE_POOL_ADDRESS=
NEXT_PUBLIC_CREATOR_SCORE_ADDRESS=
NEXT_PUBLIC_ERC8004_REGISTRY_ADDRESS=
NEXT_PUBLIC_X402_GATE_ADDRESS=

# === Optional: Monitoring ===
SENTRY_DSN=
LOG_LEVEL=info
